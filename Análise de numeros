def par(x):
    if x % 2 == 0:
        print('par')
    else:
        return 'impar'

def primo(x):
    divisor = 2
    primo = True
    while divisor < x / 2:
        if x % divisor == 0:
            primo = False
        divisor = divisor + 1
    if primo == True:
        return 'Primo'
    else:
        return 'Nao Ã© primo'

def fatorial(x):
    fatorial = 1
    for i in range(1, x + 1):
        fatorial = fatorial * i
    return fatorial

def tabuada(x):
    multiplos = []
    for i in range(2, x):
       if x % i == 0:
           multiplos.append(i)
    return multiplos
           

num = int(input('Digite um numero: '))

print(par(num))
print(primo(num))
print(fatorial(num))
print(tabuada(num))
